{
	"extends": "../tsconfig.json",
	"compilerOptions": {
		"outDir": "../out-tsc/app",
		"module": "es2015",
		"baseUrl": "./",
		"typeRoots": ["node_modules/@types"],
		"types": ["node", "reflect-metadata", "googlemaps"]
	},
	"exclude": [
		"stories",
		"**/*.stories.ts",
		"test.ts",
		"**/*.spec.ts",
		"../node_modules/@nebular/**/*.spec.ts",
		"../node_modules/angular2-wizard/**/*.spec.ts"
	],
	"include": [
		"../src/*.ts",
		"../src/**/*.ts",
		"../node_modules/@nebular/**/*.ts",
		"../node_modules/angular2-wizard/**/*.ts",
		"../node_modules/angular2-logger/app/core/level.ts",
		"../node_modules/angular2-logger/app/core/logger.ts",
		"../node_modules/angular2-logger/app/core/providers.ts"
	]
}
